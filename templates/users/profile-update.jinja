{% extends 'base.jinja' %}

{% block content %}
  <form method="post" enctype="multipart/form-data">
    <input type="hidden" name="csrfmiddlewaretoken" value="{{ csrf_token }}">
    <div class="p-10">
      {{ form }}
      <button class="btn btn-primary" type="submit">Login</button>
    </div>
  </form>
{% endblock %}
